{
  "name": "nature-server",
  "version": "0.0.1",
  "scripts": {
    "deploy": "PRISMA_ENDPOINT=http://prisma:4466 prisma deploy",
    "deploy:dev": "cross-env PRISMA_ENDPOINT=http://localhost:4466 prisma deploy",
    "dev": "yarn generate && cross-env NODE_ENV=development ts-node-dev -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "generate": "prisma generate && graphql-codegen",
    "start": "yarn deploy && yarn generate && NODE_ENV=production ts-node -T -r dotenv-flow/config -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@graphql-codegen/cli": "1.10.0",
    "@graphql-codegen/typescript": "1.10.0",
    "@graphql-codegen/typescript-resolvers": "1.10.0",
    "apollo-server-express": "2.9.15",
    "ts-node": "8.5.4",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.4",
    "prisma": "1.34.10",
    "prisma-binding": "2.3.16",
    "dotenv-flow": "3.1.0",
    "cross-env": "6.0.3"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/node": "~10.0.0",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "prettier-package-json": "2.1.3",
    "ts-node-dev": "1.0.0-pre.44"
  }
}
